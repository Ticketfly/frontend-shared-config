{
  "excludeFiles": [
    "node_modules/**",
    "bower_components",
    "<%= dasherizedPackageName %>/initializers/export-application-global.js",
    "<%= dasherizedPackageName %>/tests/helpers/start-app.js",
    "blanket-options.js"
  ],
  "additionalRules": [ "lib/jscs-rules/*.js" ],
  "fileExtensions": [".js"],
  "esnext": true,
  "disallowEmptyBlocks": true,
  "disallowMultipleSpaces": true,
  "disallowKeywordsOnNewLine": ["else"],
  "disallowMixedSpacesAndTabs": true,
  "disallowMultipleLineBreaks": true,
  "disallowMultipleLineStrings": true,
  "disallowMultipleVarDeclWithAssignment": true,
  "disallowSpacesBeforeSemicolons": true,
  "disallowSpacesInsideRoundBracesInCallExpression": true,
  "disallowOperatorBeforeLineBreak": ["."],
  "disallowPaddingNewlinesBeforeKeywords": [
    "do",
    "for",
    "else",
    "switch",
    "case",
    "try",
    "catch",
    "void",
    "while",
    "with",
    "typeof"
   ],
   "disallowPaddingNewlinesInBlocks": true,
   "disallowSpaceAfterObjectKeys": true,
   "disallowSpaceAfterPrefixUnaryOperators": ["++", "--", "+", "-", "~", "!"],
   "disallowSpaceBeforePostfixUnaryOperators": ["++", "--"],
   "disallowSpacesInCallExpression": true,
   "disallowSpacesInsideParentheses": true,
   "disallowTrailingComma": true,
   "disallowTrailingWhitespace": true,
   "disallowYodaConditions": true,
   "requireCamelCaseOrUpperCaseIdentifiers": true,
   "requireCapitalizedConstructors": true,
   "requireCommaBeforeLineBreak": true,
   "requireCurlyBraces": [
     "if",
     "else",
     "for",
     "while",
     "do",
     "try",
     "catch"
  ],
  "requireFunctionDeclarations": true,
  "requireLineBreakAfterVariableAssignment": true,
  "requireOperatorBeforeLineBreak": [
    "?",
    "=",
    "+",
    "-",
    "/",
    "*",
    "==",
    "===",
    "!=",
    "!==",
    ">",
    ">=",
    "<",
    "<="
  ],
  "requireSpacesAfterClosingParenthesisInFunctionDeclaration": {
    "beforeOpeningRoundBrace": false,
    "beforeOpeningCurlyBrace": true
  },
  "requirePaddingNewlinesBeforeKeywords": [
    "do",
    "for",
    "if",
    "switch",
    "case",
    "try",
    "catch",
    "void",
    "while",
    "with",
    "typeof"
  ],
  "requireSpaceAfterBinaryOperators": [
    "=",
    ",",
    "+",
    "-",
    "/",
    "*",
    "==",
    "===",
    "!=",
    "!=="
  ],
  "requireSpaceAfterKeywords": [
    "do",
    "for",
    "if",
    "else",
    "switch",
    "case",
    "try",
    "catch",
    "void",
    "while",
    "with",
    "return",
    "typeof"
  ],
  "requireSpaceAfterLineComment": { "allExcept": ["#", "="] },
  "requireSpaceBeforeBinaryOperators": [
    "=",
    "+",
    "-",
    "/",
    "*",
    "==",
    "===",
    "!=",
    "!=="
  ],
  "requireSpaceBeforeBlockStatements": true,
  "requireSpaceBeforeKeywords": [
    "else",
    "while",
    "catch"
  ],
  "requireSpaceBetweenArguments": true,
  "requireSpacesInConditionalExpression": {
    "afterTest": true,
    "beforeConsequent": true,
    "afterConsequent": true,
    "beforeAlternate": true
  },
  "requireSpacesInForStatement": true,
  "requireSpacesInFunctionExpression": {
    "beforeOpeningCurlyBrace": true
  },
  "requireBlocksOnNewline": 1,
  "validateIndentation": 2,
  "validateQuoteMarks": { "mark": "'", "escape": true }
}
